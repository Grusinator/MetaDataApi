                    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#decimal"/>
                    <skos:definition xml:lang="en">The (numeric) value indicating position within a temporal coordinate system </skos:definition>
                  </owl:DatatypeProperty>
                </owl:onProperty>

              </owl:Restriction>
            </rdf:first>

            <rdf:rest>
              <rdf:Description>
                <rdf:first>
